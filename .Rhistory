CA_commute %>%
filter(reportyear == "2008-2010") %>%
filter(mode %in% c("ATHOME", "BICYCLE", "PUBLICTR", "WALK", "CARPOOL") ) %>%
filter(geoname %in% c("Los Angeles city", "San Francisco city")) %>%
ggplot(aes(x = mode_name, y = percent, fill = geoname)) +
geom_col(position = position_dodge2(preserve = "total")) +
coord_polar() +
labs(x = "Transportation Mode",y = "Percent", title = "San Francisco vs Los Angeles Commuters",
subtitle = "San Franciscans are much more likely to use any non-car mode") +
theme(axis.text.x = element_text(angle=45,hjust = "inward", vjust = "inward")) +
theme_light()
